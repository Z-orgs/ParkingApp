<body>
    
    <h2>Danh sách phương tiện</h2>
    <div class="table-wrapper">
        <div class="searchVehicle">
            <form method="post">
                <input type="text" name="keywordLicense" placeholder="license">
                <button type="submit">tìm</button>
            </form>
        </div>
        <table class="fl-table">
            <thead>
                <tr>
                    <th>ID(Vehicle)</th>
                    <th>License</th>
                    <th>Type</th>
                    <th>Owner by ID</th>
                    <th>Actions</th>
                </tr>
            </thead>
            <tbody>
                <% for(var i=0; i < dataVehicle.length; i++) { %>
                    <tr>
                        <td>
                            <%= dataVehicle[i].idV %>
                        </td>
                        <td>
                            <%= dataVehicle[i].license %>
                        </td>
                        <td>
                            <%= dataVehicle[i].type %>
                        </td>
                        <td>
                            <%= dataVehicle[i].id %>
                        </td>
                        <td>
                            <a href="/detail/vehicle/<%= dataVehicle[i].idV%>">Detail</a>
                            <form action="edit-vehicle/<%= dataVehicle[i].idV%>"><button type="submit">Edit</button></form>
                            <form action="delete-vehicle" method="post">
                                <input type="text" name="idV" hidden value="<%= dataVehicle[i].idV%>">
                                <button type="submit">Delete</button>
                            </form>
                        </td>
                    </tr>
                    <% } %>
            </tbody>
        </table>
    </div>
    
